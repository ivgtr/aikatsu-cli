{
  "name": "aikatsu-cli",
  "description": "アイカツ！格言を再生するCLI",
  "keywords": [
    "aikatsu",
    "kakugen",
    "cli"
  ],
  "version": "1.2.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "aikatsu": "./dist/cli.js"
  },
  "engines": {
    "node": ">= 12.2.0"
  },
  "author": {
    "name": "ivgtr",
    "email": "ivgtr.me@gmail.com",
    "url": "https://github.com/ivgtr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivgtr/aikatsu-cli.git"
  },
  "homepage": "https://github.com/ivgtr/aikatsu-cli#readme",
  "bugs": {
    "url": "https://github.com/ivgtr/aikatsu-cli/issues"
  },
  "files": [
    "dist",
    "src/index.ts",
    "src/cli.ts",
    "src/configs/kakugen.js"
  ],
  "scripts": {
    "start": "tsc -w",
    "type-check": "tsc --noEmit",
    "type-build": "tsc --project ./tsconfig-build.json --emitDeclarationOnly",
    "prebuild": "node -e 'fs.rmSync(`dist`, {recursive:true, force:true})'",
    "build": "rollup -c && npm run type-build",
    "format": "prettier -w ./src/*",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/preset-env": "7.15.6",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-typescript": "8.2.5",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.1",
    "@types/update-notifier": "5.1.0",
    "eslint": "7.32.0",
    "eslint-config-sumikko": "1.2.2",
    "jest": "27.1.1",
    "prettier": "2.3.2",
    "rollup": "2.56.3",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-preserve-shebang": "1.0.1",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.2"
  },
  "dependencies": {
    "meow": "10.1.1",
    "symphogear-g": "^0.4.2",
    "update-notifier": "^5.1.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "preset": "ts-jest/presets/js-with-ts-esm",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
